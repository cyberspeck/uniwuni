#! /usr/bin/python

#from math import *

import math,sys

fp=open("a.dat","r")

data=fp.readlines()

fp.close()

print data

zl=[]

for z in data:
   #print z
   t=[float(x) for x in z.split()]
   zl.append(t)
   #print t

print "zl=",zl

out=open("out.dat","w")
print out
for t in zl:
   print t
   w=math.sqrt(t[1])
   s= "%7.3f %12.7f\n" % (t[0],w)
   out.write(s)
   out.write(str(t[0])+"\t"+str(w)+'\n')

out.close()
